"Instruções SQL utilizadas na criação das tabelas, corrigido relação atráves de Foreign Keys após a criação das tabelas"

CREATE TABLE checklist_app.diagnostic(
	ID SERIAL NOT NULL PRIMARY KEY,
	MODULES VARCHAR (30)
);

CREATE TABLE checklist_app.exterior(
	ID SERIAL NOT NULL PRIMARY KEY,
	BODY_BUMPER INT,
	DOORS_HOOD_TAILGATE INT,
	GRILLE_TRIM_ROOF_RACK INT,
	GLASS_OUTSIDEMIRROR INT,
	LIGHTS INT
);

CREATE TABLE checklist_app.body_bumper(
	ID SERIAL NOT NULL PRIMARY KEY,
	FLOOD_DAMAGE SMALLINT,
	FIRE_DAMAGE SMALLINT,
	MAJOR_DAMAGE SMALLINT,
	BODY_PANEL SMALLINT,
	BUMPER INT
);

CREATE TABLE checklist_app.doors_hood_tailgate(
	ID SERIAL NOT NULL PRIMARY KEY,
	DOORS_ALIGNMENT SMALLINT,
	DOOR_HINGES SMALLINT,
	ROOF_INSPECTION SMALLINT,
	HOOD_ALIGNMENT SMALLINT,
	HOOD_RELEASE SMALLINT,
	HOOD_HINGES SMALLINT,
	HOOD_GASSTRUTS SMALLINT,
	TAILGATE_ALIGNMENT SMALLINT,
	TRUNKGASSTRUTS SMALLINT,
	POWERLIFT_GATE_OPERATION SMALLINT
);

CREATE TABLE checklist_app.glass_outside_mirrors(
	ID SERIAL NOT NULL PRIMARY KEY,
	WINDSHIELD SMALLINT,
	SIDE_GLASS SMALLINT,
	REAR_WINDOW SMALLINT,
	WIPER_BLADE SMALLINT,
	OUTSIDE_MIRROR SMALLINT,
	OUTSIDE_MIRROR_FOLDING SMALLINT
);

CREATE TABLE checklist_app.grille_trim_roof_rack(
	ID SERIAL NOT NULL PRIMARY KEY,
	GRILLE SMALLINT,
	TRIM_INSPECTION SMALLINT,
	ROOF_RACK SMALLINT
);

CREATE TABLE checklist_app.exterior_lights(
	ID SERIAL NOT NULL PRIMARY KEY,
	FRONT_LIGHTS SMALLINT,
	BACK_LIGHTS SMALLINT,
	SIDE_LIGHTS SMALLINT,
	HAZARD_LIGHTS SMALLINT,
	AUTO_ONOFF_LIGHTING SMALLINT,
	TRAILER_CAMP_CONNECTOR SMALLINT
);

CREATE TABLE checklist_app.hybrid(
	ID SERIAL NOT NULL PRIMARY KEY,
	COOLING_SYSTEM SMALLINT,
	POWERTRAIN_MOUNT SMALLINT,
	INFORMATION_DISPLAY SMALLINT,
	POWER_OUTLET SMALLINT
);

CREATE TABLE checklist_app.airbag_safety_belts(
	ID SERIAL NOT NULL PRIMARY KEY,
	AIRBAGS SMALLINT,
	SAFETY_BELTS SMALLINT
);

CREATE TABLE checklist_app.audio_alarm(
	ID SERIAL NOT NULL PRIMARY KEY,
	RADIO_CD_SPEAKER SMALLINT,
	ANTENNA SMALLINT,
	ALARMA_SYSTEM SMALLINT,
	NAVIGATION_SYSTEM SMALLINT
);

CREATE TABLE checklist_app.carpet_trim_mats(
	ID SERIAL NOT NULL PRIMARY KEY,
	INTERIOR_FREE_ODODR SMALLINT,
	CARPET SMALLINT,
	FLOOR_MATS SMALLINT,
	DOOR_TRIM_PANELS SMALLINT,
	HEADLINER SMALLINT
);

CREATE TABLE checklist_app.heat_ac_defog_defrost(
	ID SERIAL NOT NULL PRIMARY KEY,
	AC_SYSTEM SMALLINT,
	HEATING_SYSTEM SMALLINT,
	DEFOG_DEFROST SMALLINT
);

CREATE TABLE checklist_app.interior (
	ID SERIAL NOT NULL PRIMARY KEY,
	AIRBAG_SAFETY_BELTS INT,
	AUDIO_ALARM INT,
	HEAT_AC_DEFOG_DEFROST INT,
	INTERIOR_AMENITIES INT,
	CARPET_TRIM_MATS INT,
	SEATS INT,
	SUNROOF_CONVERTIBLE_TOP INT,
	WINDOWS_DOOR_LOCKS INT,
	LUGGAGE_COMPARTMENT INT
);

CREATE TABLE checklist_app.interior_amenities(
	ID SERIAL NOT NULL PRIMARY KEY,
	CLOCK SMALLINT,
	TILT_TELESCOPIC_STEERING_WHEEL SMALLINT,
	STEERING_COLUMN_LOCK SMALLINT,
	HORN SMALLINT,
	WARNING_CHIMES SMALLINT,
	INSTRUMENT_PANEL_WARNING_LIGHTS SMALLINT,
	WINDSHIEL_WIPERS SMALLINT,
	WASHERS SMALLINT,
	INTERIOR_COURTESY_DOME_LIGHTS SMALLINT,
	REAR_VIEW_MIRROR SMALLINT,
	AUTO_DIMING_REAR_VIEW_MIRROR SMALLINT,
	BLIS SMALLINT,
	REAR_VIEW_CAMERA_SYSTEM SMALLINT,
	ACTIVE_PARK_ASSIST SMALLINT,
	REAR_ENTERTAINMENT_SYSTEM SMALLINT,
	POWER_OUTLETS SMALLINT,
	LIGHTER SMALLINT,
	ASHTRAYS SMALLINT,
	GLOVEBOX SMALLINT,
	CENTER_ARM_REST_CONSOLE SMALLINT,
	SUNVISORS SMALLINT,
	ADJUSTABLE_PEDALS SMALLINT
);

CREATE TABLE checklist_app.luggage_compartment(
	ID SERIAL NOT NULL PRIMARY KEY,
	LUGGAGE_CARPET SMALLINT,
	LUGGAGE_TRIM SMALLINT,
	LUGGAGE_LIGHT SMALLINT,
	JACK_TOOL_KIT SMALLINT,
	SPARE_TIRE_INSPECTION SMALLINT,
	SPARE_TIRE_AIRPRESSURE SMALLINT,
	TIRE_INFLATOR_KIT SMALLINT,
	EMERGENCY_TRUNK_LID SMALLINT
);


CREATE TABLE checklist_app.seats(
	ID SERIAL NOT NULL PRIMARY KEY,
	UPHOLSTERY SMALLINT,
	SEAT_HEAD_RESTRAINT_ADJUSTMENT SMALLINT,
	FOLDING_SEATS SMALLINT,
	HEATED_SEATS SMALLINT,
	COOLED_SEATS SMALLINT,
	INTEGRATED_CHILD_SAFETY_SEATS SMALLINT
);

CREATE TABLE checklist_app.sunroof_convertible_top(
	ID SERIAL NOT NULL PRIMARY KEY,
	SUNROOF SMALLINT,
	CONVERTIBLE_TOP SMALLINT
);

CREATE TABLE checklist_app.windows_door_locks(
	ID SERIAL NOT NULL PRIMARY KEY,
	DOOR_HANDLES_RELEASE SMALLINT,
	REMOTE_ENTRY SMALLINT,
	PUSH_BUTTON_START SMALLINT,
	DOOR_LOCKS SMALLINT,
	CHILD_SAFETY_LOCKS SMALLINT,
	WINDOW_CONTROLS SMALLINT,
	REMOTE_DECKLID_RELEASE SMALLINT,
	FUEL_FILLER_DOOR_RELEASE SMALLINT
);

CREATE TABLE checklist_app.road(
	ID SERIAL NOT NULL PRIMARY KEY,
	ENGINE_STARTS SMALLINT,
	ENGINE_IDLE SMALLINT,
	REMOTE_START_OPERATION SMALLINT,
	ENGINE_ACCELERATE_CRUISE SMALLINT,
	ENGINE_NOISE_COLD_HOT SMALLINT,
	TRANSMISSION_SHIFT_OPERATION SMALLINT,
	TRANSMISSION_NOISE_COLD_HOT SMALLINT,
	SHIFT_INTERLOCK_OPERATION SMALLINT,
	DRIVE_AXLE_TRANSFER_CASE_NOISE SMALLINT,
	CLUTCH_OPERATION SMALLINT,
	STEERING_OPERATION SMALLINT,
	BODY_SUSPENSION_SQUEAKS SMALLINT,
	STRUTS_SHOCKS_OPERATION SMALLINT,
	BRAKE_ABS_OPERATION SMALLINT,
	CRUISE_CONTROL SMALLINT,
	GAUGES_OPERATION SMALLINT,
	DRIVER_SELECT_MEMORY_SYSTEM SMALLINT,
	ABNORMAL_WIND_NOISE SMALLINT
);

CREATE TABLE checklist_app.brake(
	ID SERIAL NOT NULL PRIMARY KEY,
	CALIPERS_WHEEL_CYLINDERS SMALLINT,
	FRONT_BRAKE_PADS_SHOES SMALLINT,
	REAR_BRAKE_PADS_SHOES SMALLINT,
	ROTORS_DRUMS SMALLINT,
	BRAKE_LINES_HOSES_FITTINGS SMALLINT,
	PARKING_BRAKE SMALLINT,
	MASTER_CYLINDER_BOOSTER SMALLINT
);

CREATE TABLE checklist_app.exhaust_system(
	ID SERIAL NOT NULL PRIMARY KEY,
	EXHAUST_SYSTEM_CONDITION SMALLINT,
	EMISSIONS_CONTROL SMALLINT
);

CREATE TABLE checklist_app.frame(
	ID SERIAL NOT NULL PRIMARY KEY,
	FRAME_DAMAGE SMALLINT,
	FUEL_SUPPLY_SYSTEM SMALLINT
);

CREATE TABLE checklist_app.tires_wheels(
	ID SERIAL NOT NULL PRIMARY KEY,
	TIRES_MATCH_SIZE SMALLINT,
	WHEELS_MATCH_SIZE SMALLINT,
	TWI_FRONT_LR SMALLINT,
	TWI_REAR_LR SMALLINT,
	TIRE_PRESSURE_FRONT_LR SMALLINT,
	TIRE_PRESSURE_REAR_LR SMALLINT,
	TIRE_PRESSURE_SYSTEM SMALLINT,
	WHEELS SMALLINT,
	WHEELS_COVERS SMALLINT,
	RACK_PINION_LINKAGE_BOOTS SMALLINT,
	CONTROL_ARMS_BUSHING_BALL_JOINTS SMALLINT,
	TIE_RODS_IDLER_ARM SMALLINT,
	SPRINGS SMALLINT,
	STRUTS_SHOCKS SMALLINT,
	WHEEL_ALIGNMENT SMALLINT,
	POWER_STEERING_PUMP SMALLINT
);

CREATE TABLE checklist_app.transmission_differential(
	ID SERIAL NOT NULL PRIMARY KEY,
	AUTOMATIC_TRANSMISSION SMALLINT,
	MANUAL_TRANSMISSION SMALLINT,
	HUB_OPERATION_4X4 SMALLINT,
	UNIVERSAL_JOINTS_CV_BOOTS SMALLINT,
	TRANSMISSION_MOUNTS SMALLINT,
	DIFFERENTIAL_DRIVE_AXLE SMALLINT
);

CREATE TABLE checklist_app.underbody(
	ID SERIAL NOT NULL PRIMARY KEY,
	FRAME INT,
	EXHAUST_SYSTEM INT,
	TRANSMISSION_DIFFERENTIAL INT,
	TIRES_WHEELS INT,
	BRAKES INT
);

CREATE TABLE checklist_app.cooling_system(
	ID SERIAL NOT NULL PRIMARY KEY,
	RADIATOR SMALLINT,
	PRESSURE_TEST_RADIATOR_CAP SMALLINT,
	COOLING_FANS SMALLINT,
	WATER_PUMP SMALLINT,
	COOLANT_RECOVERY_TANK SMALLINT,
	CABIN_AIR_FILTER SMALLINT
);

CREATE TABLE checklist_app.engine_electrical_system(
	ID SERIAL NOT NULL PRIMARY KEY,
	STARTER SMALLINT,
	IGNITION SMALLINT,
	BATTERY SMALLINT,
	ALTERNATOR_OUTPUT SMALLINT,
	DIESEL_GLOW_PLUG_SYSTEM SMALLINT
);

CREATE TABLE checklist_app.engine(
	ID SERIAL NOT NULL PRIMARY KEY,
	FLUID_LEAKS SMALLINT,
	HOSES_LINES SMALLINT,
	BELTS SMALLINT,
	WIRING SMALLINT,
	WATER_COOLANT_ENGINE_OIL SMALLINT,
	OIL_PRESSURE SMALLINT,
	CYLINDER_COMPRESSION SMALLINT,
	TIMING_BELTS SMALLINT,
	ENGINE_MOUNTS SMALLINT,
	TURBOCHARGER SMALLINT
);

CREATE TABLE checklist_app.fluids(
	ID SERIAL NOT NULL PRIMARY KEY,
	ENGINE_OIL_FILTER SMALLINT,
	ENGINE_OIL SMALLINT,
	COOLANT SMALLINT,
	BRAKE_FLUID SMALLINT,
	AUTOMATIC_TRANSMISSION_FLUID SMALLINT,
	TRANSFERCASE_FLUID SMALLINT,
	DRIVEAXLE_FLUID SMALLINT,
	POWERSTEERING_FLUID SMALLINT,
	MANUAL_TRANSMISSION_FLUID SMALLINT,
	HYDRAULIC_CLUTCH_FLUID SMALLINT,
	WASHER_FLUID SMALLINT
);

CREATE TABLE checklist_app.fuel_system(
	ID SERIAL NOT NULL PRIMARY KEY,
	FUEL_PUMP_NOISE SMALLINT,
	FUEL_PUMP_PRESSURE SMALLINT,
	FUEL_FILTER SMALLINT,
	ENGINE_AIR_FILTER SMALLINT
);

CREATE TABLE checklist_app.underhood(
	ID SERIAL NOT NULL PRIMARY KEY,
	FLUIDS INT,
	ENGINE INT,
	COOLING_SYSTEM INT,
	FUEL INT,
	ENGINE_ELECTRICAL_SYSTEM INT
);